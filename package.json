{
  "name": "minista-project",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "panda codegen",
    "dev": "npm run watch:components-export-list & minista",
    "build": "npm run generate:components-export-list && minista build",
    "preview": "minista preview",
    "prod": "npm run prepare && npm run build",
    "storybook": "npx panda cssgen && storybook dev -p 6006",
    "build-storybook": "storybook build",
    "generate:components-export-list": "node --loader ts-node/esm lib/component-list.ts init",
    "watch:components-export-list": "node --loader ts-node/esm lib/component-list.ts watch",
    "generate-component": "npx scaffdog generate",
    "lint": "tsc --noEmit -p tsconfig.json && npx eslint . && npx markuplint --config .markuplintrc.yml src/**/*.tsx && npx prettier -w ."
  },
  "devDependencies": {
    "@markuplint/jsx-parser": "^3.9.0",
    "@markuplint/react-spec": "^3.10.0",
    "@pandacss/dev": "^0.10.0",
    "@storybook/addon-essentials": "^7.2.2",
    "@storybook/addon-interactions": "^7.2.2",
    "@storybook/addon-links": "^7.2.2",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.2.2",
    "@storybook/react": "^7.2.2",
    "@storybook/react-vite": "^7.2.2",
    "@storybook/testing-library": "^0.2.0",
    "@tsconfig/recommended": "^1.0.2",
    "@types/react": "^18.2.16",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "css-has-pseudo": "^6.0.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^38.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.1",
    "markuplint": "^3.12.0",
    "minista": "^3.1.4",
    "prettier": "3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "scaffdog": "^3.0.0",
    "storybook": "^7.2.2",
    "tslint": "^6.1.3",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@types/chokidar": "^2.1.3",
    "@types/node": "^20.4.10",
    "@typescript-eslint/parser": "^6.4.1",
    "chokidar": "^3.5.3",
    "htmx": "^0.0.2",
    "htmx.org": "^1.9.4",
    "ts-node": "^10.9.1",
    "ts-pattern": "^5.0.5"
  }
}
